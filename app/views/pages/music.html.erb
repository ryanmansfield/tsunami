<div class="music">
  <!-- Hero -->
  <%= render 'shared/banner' %>

  <!-- Content -->

  <div class="container">
    <h1>Listen Here</h1>
    <div class="players">
      <div class="player soundcloud">
        <iframe width="70%" height="400" scrolling="no" frameborder="no" allow="autoplay" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/users/1626228&color=%23ff5500&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true&visual=true"></iframe>
      </div>
      <div class="player spotify">
        <iframe src="https://open.spotify.com/embed/artist/5UqnbnezI2TvaTWKwYhyrP" width="300" height="400" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
      </div>
    </div>
  </div>

  <%= render 'shared/footer'  %>
</div>
